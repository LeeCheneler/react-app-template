{
  "name": "react-app-template",
  "version": "1.0.0",
  "description": "React app template.",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "yarn test && yarn lint && webpack",
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx && stylelint \"*/**/*.scss\" --syntax scss",
    "stats": "webpack --profile --json > stats.json"
  },
  "dependencies": {
    "@pluto-css/foundation": "^1.2.2",
    "babel-polyfill": "^6.26.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "offline-plugin": "^4.8.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2"
  },
  "author": "leecheneler",
  "repository": "https://github.com/react-app-template",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "16",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-nebula": "^0.2.0",
    "svg-sprite-loader": "^3.2.6",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/__mocks__/svgMock.js"
    },
    "setupFiles": [
      "./jestSetup.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/jestPolyfills.js"
    ]
  }
}
